<!DOCTYPE validators PUBLIC
  		"-//Apache Struts//XWork Validator 1.0.3//EN"
  		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<!--创建时间:2015年4月7日 -->
<validators>
	<field name="nick">
		<field-validator type="requiredstring">
			<message>請填入昵称</message>
		</field-validator>
	</field>
	<field name="username">
		<field-validator type="requiredstring">
			<message>請填入用戶名</message>
		</field-validator>
	</field>
	<field name="password">
		<field-validator type="requiredstring">
			<message>請填入密碼</message>
		</field-validator>
	</field>
	<field name="education">
		<field-validator type="requiredstring">
			<message>請选择学历</message>
		</field-validator>
	</field>
	<field name="telephone">
		<field-validator type="requiredstring">
			<message>請输入电话号码</message>
		</field-validator>
	</field>
</validators>